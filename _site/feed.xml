  &lt;span class=&quot;kd&quot;&gt;var&lt;/span&gt; &lt;span class=&quot;nx&quot;&gt;apiUrl&lt;/span&gt; &lt;span class=&quot;o&quot;&gt;=&lt;/span&gt; &lt;span class=&quot;s1&quot;&gt;&#39;http://58ae85af73918a1200362d5f.mockapi.io/myTodos/duty&#39;&lt;/span&gt;
  &lt;span class=&quot;c1&quot;&gt;//get data&lt;/span&gt;
